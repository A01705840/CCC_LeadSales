                        <% for (let version of versiones) { %>
                        <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                            <%= version.NombreVersion %>
                        </th>
                        <td class="px-6 py-4">
                            <%= version.IDVersion %>
                        </td>
                        <td class="px-6 py-4">
                            <%= version.IDUsuario %>
